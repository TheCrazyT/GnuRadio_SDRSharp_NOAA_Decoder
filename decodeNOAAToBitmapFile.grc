  <block>
    <key>epy_block</key>
    <param>
      <key>alias</key>
      <value></value>
    </param>
    <param>
      <key>_io_cache</key>
      <value>('To bmp File', 'blk', [('width', '800'), ('height', '600'), ('path', "''")], [('0', 'byte', 1), ('1', 'byte', 1)], [], 'arguments to this function show up as parameters in GRC', ['height', 'path', 'width'])</value>
    </param>
    <param>
      <key>_source_code</key>
      <value>
import numpy as np
from gnuradio import gr
WORK_CALLED_PRODUCE=-2
WORK_DONE=-1

class blk(gr.sync_block):  # other base classes are basic_block, decim_block, interp_block

    def __init__(self,width=800,height=600,path=""):  # only default arguments here
        """arguments to this function show up as parameters in GRC"""
        gr.sync_block.__init__(
            self,
            name='To bmp File',  
            in_sig=[np.byte,np.byte],
            out_sig=[]
        )
        self.width = width
        self.height = height
        self.stopat = width*height
        self.path = path
        self.img = np.zeros(shape=(width*height),dtype=np.uint8)
        self.pos = 0
        self.wroteFile = False

    def work(self, input_items, output_items):
        if(self.path != ""):
            if(self.pos &lt; self.stopat):
                ii = input_items[0][input_items[1]==1]
                li = len(ii)
                ishape = self.img[self.pos:self.pos+li].shape
                if ishape[0] &lt; li:
                    li = ishape[0]
                    ii = ii[0:li]
                if li &gt; 0:
                    self.img[self.pos:self.pos+li] = ii
                    self.pos += li
            else:
                if not self.wroteFile:
                    self.wroteFile = True
                    img = np.maximum(np.minimum(self.img,np.ones(self.img.shape)*255),np.zeros(self.img.shape))
                    img = img.reshape(self.height,self.width)
                    import scipy.misc
                    scipy.misc.imsave(self.path, img)
                    return WORK_DONE
        return li
